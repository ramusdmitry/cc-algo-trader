[MASTER]
max-line-length = 160

[FORMAT]
indent-string='    '

[BASIC]
good-names=i,j,k,ex,Run,_

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    line-too-long,
    too-many-instance-attributes,
    too-many-arguments,
    too-many-locals,
    too-many-public-methods,
    too-many-branches,
    too-many-statements,
    too-few-public-methods
